  "type": "library",
  "description": "Beauty OpenAPI Support",
  "keywords": ["beauty", "openapi", "support"],
  "license": "MIT",
  "authors": [],
  "version": "1.0.0",
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.1",
    "beauty-framework/cli": "^1.0",
    "zircote/swagger-php": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "Beauty\\OpenApi\\": "src/"
    }
  },
  "require-dev": {
